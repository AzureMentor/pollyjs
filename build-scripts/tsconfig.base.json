{
  "compilerOptions": {
    "moduleResolution": "node",
    "downlevelIteration": true,
    "sourceMap": true,
    "removeComments": false,
    "forceConsistentCasingInFileNames": true,
    "module": "es2015",
    "target": "es5",
    "lib": ["es2015", "es2016", "es2017"],
    "stripInternal": true,
    "allowSyntheticDefaultImports": true,
    "strict": true
  }
}
