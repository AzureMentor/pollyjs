{
  "name": "@pollyjs/preset-browser",
  "version": "0.0.0",
  "description": "Shared utilities and constants between @pollyjs packages",
  "main": "dist/cjs/pollyjs-preset-browser.js",
  "module": "dist/es/pollyjs-preset-browser.js",
  "browser": "dist/umd/pollyjs-preset-browser.js",
  "files": [
    "src",
    "dist"
  ],
  "repository": "https://github.com/netflix/pollyjs/tree/master/packages/@pollyjs/preset-browser",
  "scripts": {
    "build": "npm-run-all clean rollup",
    "test:build": "rollup -c rollup.config.test.js",
    "clean": "rimraf dist",
    "rollup": "rollup -c",
    "rollup:prod": "NODE_ENV=production yarn rollup",
    "watch": "yarn rollup -w",
    "format": "prettier --config ../../../.prettier.js --write **/*.js",
    "lint": "eslint ./*.js src",
    "prepublishOnly": "npm-run-all clean --parallel rollup rollup:prod"
  },
  "keywords": [
    "polly",
    "pollyjs",
    "preset-browser"
  ],
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    {
      "name": "Jason Mitchell",
      "email": "jason.mitchell.w@gmail.com"
    },
    {
      "name": "Offir Golan",
      "email": "offirgolan@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@pollyjs/adapter-fetch": "^1.0.5",
    "@pollyjs/adapter-xhr": "^1.0.5",
    "@pollyjs/core": "^1.1.4",
    "@pollyjs/persister-local-storage": "^1.0.5",
    "@pollyjs/persister-rest": "^1.0.5"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2"
  }
}
